@model FujiyBlog.Core.DomainObjects.Post
@{
    ViewBag.Title = Model.Title;
    Layout = "~/Views/Shared/_Layout.cshtml";
}
<div id="postnavigation">
    Titulo do post anterior | Titulo do proximo post
</div>

@Html.DisplayForModel()

@{Html.RenderPartial(MVC.Post.Views.Comments, Model.Comments);}

@{Html.RenderPartial(MVC.Post.Views.DoComment, new FujiyBlog.Core.DomainObjects.PostComment { Post = Model });}