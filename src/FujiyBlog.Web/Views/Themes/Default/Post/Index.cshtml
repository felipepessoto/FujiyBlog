@model FujiyBlog.Web.ViewModels.PostIndex
@{
    Layout = "../Shared/_Layout.cshtml";
}
@foreach (var item in Model.RecentPosts)
{
    Html.RenderPartial(MVC.Themes.Views.Default.Post.Post, item);
}
@Pager.Links(Model.CurrentPage, Model.TotalPages, Resources.Labels.PreviousPosts, Resources.Labels.NextPosts)
